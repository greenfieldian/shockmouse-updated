{
  "private": true,
  "name": "starter-ui",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf node_modules coverage reports public .cache",
    "deploy": "spraoi deploy-ui -c $config",
    "format": "yarn svgo && yarn prettier",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --ignore-path .gitignore \"src/**/*.js\"",
    "lint:styles": "stylelint --ignore-path .gitignore \"src/**/*.js\"",
    "prettier": "prettier --write --ignore-path .gitignore \"**/*.{js,gql,json,md,yml}\"",
    "serve": "gatsby serve --port 8000",
    "start": "gatsby develop",
    "svgo": "GLOBIGNORE=*limra-logo.svg && svgo --config node_modules/@spraoi/svgo-config/svgo.yml src/images/**/*.svg",
    "test": "jest --ci --runInBand"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/runtime": "^7.16.0",
    "@reach/router": "^1.3.4",
    "@spraoi/auth": "^0.29.57",
    "@spraoi/babel-preset": "^0.29.0",
    "@spraoi/base": "^0.29.59",
    "@spraoi/gatsby-config": "^0.29.39",
    "@spraoi/helpers": "^0.29.43",
    "@spraoi/scripts": "^0.29.26",
    "@spraoi/svgo-config": "^0.29.0",
    "@spraoi/validations": "^0.29.0",
    "@types/react": "^17.0.34",
    "babel-core": "^7.0.0-bridge",
    "babel-plugin-styled-components": "^1.13.3",
    "babel-preset-gatsby": "^2.1.0",
    "final-form": "^4.20.4",
    "gatsby": "^4.1.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-catch-links": "^4.1.0",
    "gatsby-plugin-extract-image-colors": "^0.0.14",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-image": "^2.1.0",
    "gatsby-plugin-manifest": "^4.1.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-react-helmet": "^5.1.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^4.1.0",
    "gatsby-plugin-sitemap": "^5.1.0",
    "gatsby-remark-dropcap": "^0.1.2",
    "gatsby-remark-images": "^6.1.0",
    "gatsby-remark-numbered-footnotes": "^1.0.1",
    "gatsby-remark-prismjs": "^6.1.0",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-source-contentful": "^6.1.0",
    "gatsby-source-filesystem": "^4.1.0",
    "gatsby-transformer-remark": "^5.1.0",
    "gatsby-transformer-sharp": "^4.1.0",
    "gatsby-transformer-yaml": "^4.1.0",
    "graphql": "^16.0.1",
    "graphql-tag": "^2.12.5",
    "lodash": "^4.17.21",
    "polished": "^4.1.3",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-burgers": "^1.3.0",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.7",
    "react-final-form-listeners": "^1.0.3",
    "react-helmet": "^6.1.0",
    "react-simple-maps": "^2.3.0",
    "react-social-sharing": "^3.3.0",
    "react-spinners": "^0.11.0",
    "styled-components": "^5.3.3",
    "svgo": "^2.8.0"
  },
  "devDependencies": {
    "@spraoi/eslint-config": "^0.29.0",
    "@spraoi/jest-config": "^0.29.1",
    "@spraoi/prettier-config": "^0.29.0",
    "@spraoi/stylelint-config": "^0.29.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "eslint": "^8.1.0",
    "jest": "^27.3.1",
    "jest-haste-map": "^27.3.1",
    "jest-junit": "^13.0.0",
    "jest-resolve": "^27.3.1",
    "prettier": "^2.4.1",
    "stylelint": "^14.0.1"
  },
  "resolutions": {
    "styled-components": "^5.2.0"
  }
}
